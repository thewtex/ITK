itk_wrap_class("itk::ImageBase" POINTER)
  set(dims "")
  foreach(d ${ITK_WRAP_DIMS})
    # For VelocityFieldTranform
    INCREMENT(d_inc ${d})
    list(APPEND dims ${d} ${d_inc})
  endforeach(d)
  list(REMOVE_DUPLICATES dims)
  foreach(d ${dims})
    itk_wrap_template("${d}" "${d}")
  endforeach(d)
itk_end_wrap_class()

itk_wrap_class("itk::Image" POINTER)
  set(WRAPPER_TEMPLATES "${itk_Wrap_Image}")
itk_end_wrap_class()
