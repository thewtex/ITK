      itk_wrap_template("GI${ITKM_${t}}${d}GI${ITKM_${t}}${d}GPUBinaryThreshold${ITKM_${t}}${ITKM_${t}}BinaryThresholdImageFilter" "itk::GPUImage< ${ITKT_${t}}, ${d} >, itk::GPUImage< ${ITKT_${t}}, ${d} >, itk::Functor::GPUBinaryThreshold< ${ITKT_${t}}, ${ITKT_${t}} >, itk::BinaryThresholdImageFilter< itk::GPUImage< ${ITKT_${t}}, ${d} >, itk::GPUImage< ${ITKT_${t}}, ${d} > >")
    endforeach()
  endforeach()
itk_end_wrap_class()

itk_wrap_class("itk::GPUBinaryThresholdImageFilter" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
